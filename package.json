{
  "name": "timeflow-monorepo",
  "private": true,
  "packageManager": "pnpm@9.15.0",
  "scripts": {
    "dev:web": "pnpm --filter @acme/web dev",
    "dev:timeflow": "pnpm --filter timeflow start",
    "gen:api": "pnpm --filter @acme/api-client gen",
    "build": "pnpm --filter @acme/api build && pnpm --filter @acme/api-client build && pnpm --filter @acme/web build",
    "typecheck": "pnpm -r typecheck"
  },
  "devDependencies": {
    "typescript": "^5.7.3"
  }
}
