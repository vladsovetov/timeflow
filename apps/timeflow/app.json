{
  "expo": {
    "name": "Timeflow",
    "slug": "timeflow",
    "version": "0.9.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "scheme": "timeflow",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#0B0B14"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.acme.timeflow"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#0B0B14"
      },
      "package": "com.acme.timeflow"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "@react-native-community/datetimepicker",
        {
          "android": {
            "datePicker": {
              "colorAccent": { "light": "#7C3AED", "dark": "#7C3AED" },
              "colorControlActivated": { "light": "#7C3AED", "dark": "#7C3AED" },
              "colorControlHighlight": { "light": "#7C3AED40", "dark": "#7C3AED40" }
            },
            "timePicker": {
              "numbersSelectorColor": { "light": "#7C3AED", "dark": "#7C3AED" },
              "headerBackground": { "light": "#121225", "dark": "#121225" }
            }
          }
        }
      ],
      [
        "expo-localization",
        {
          "supportedLocales": {
            "ios": ["en", "uk", "ru"],
            "android": ["en", "uk", "ru"]
          }
        }
      ],
      "expo-router",
      [
        "expo-secure-store",
        {
          "configureAndroidBackup": true,
          "faceIDPermission": "Allow $(PRODUCT_NAME) to access your Face ID biometric data."
        }
      ],
      "expo-web-browser",
      [
        "expo-updates",
        {
          "username": null
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "updates": {
      "enabled": false,
      "checkAutomatically": "NEVER",
      "fallbackToCacheTimeout": 0
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "2f221b61-5d70-4d73-965e-b7118edfb77d"
      }
    }
  }
}
